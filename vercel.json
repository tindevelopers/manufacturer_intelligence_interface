{
  "version": 2,
  "buildCommand": "yarn build",
  "outputDirectory": ".next",
  "installCommand": "yarn install",
  "framework": "nextjs",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "ABACUS_API_KEY": "@abacus_api_key",
    "ABACUSAI_API_KEY": "@abacusai_api_key"
  }
}
